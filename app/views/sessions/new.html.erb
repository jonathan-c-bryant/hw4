<h1 class="text-uppercase text-center pt-4">Login</h1>

<%= form_with url: "/sessions" do |form| %>
  <p>
    <%= form.label "email" %>
    <%= form.text_field "email" %>
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </p>
  
  <p>
    <button type="submit" class="btn btn-dark">Login</button>
  </p>
<% end %>